    LightningAxe.name = "Lightning Axe";
    LightningAxe.damage = 300;
    LightningAxe.Weight = 65;
    LightningAxe.Rarity = "Rare";
    LightningAxe.Description = "The deadliest weapon";
    LightningAxe.Killerweapon();

    Weapon Odinmachinegun = new Weapon();

    Odinmachinegun.object = "MachineGun";
    Odinmachinegun.name = "Odin";
    Odinmachinegun.damage = 400;
    Odinmachinegun.Weight = 70;
    Odinmachinegun.Rarity = "Rare";
    Odinmachinegun.Description = "This kind of machinegun is too heavy and not easy to use";
